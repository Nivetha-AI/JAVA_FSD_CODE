package Conne;

import org.hibernate.SessionFactory;
import org.hibernate.cfg.Configuration;

;

public class Connt {
	private static SessionFactory fac;
	public Connt()
	{
		fac=new Configuration().configure("hiber.cfg.xml").addAnnotatedClass(Login.class).addAnnotatedClass(Myorder.class).buildSessionFactory();
		
	}
}
